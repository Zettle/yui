{
    "name": "vue-yui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve",
        "build:sit": "vue-cli-service build --mode sit",
        "build:uat": "vue-cli-service build --mode uat",
        "build:prod": "vue-cli-service build",
        "analy": "vue-cli-service build --mode sit --report",
        "analy:json": "vue-cli-service build --mode sit --report-json",
        "mkfont": "node ./svg/app.js && node ./svg/copy.js",
        "lint": "vue-cli-service lint",
        "fix": "stylelint **/*.vue --config stylelint.config.js"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "lib-flexible": "^0.3.2",
        "vue": "^2.6.6",
        "vue-router": "^3.0.1",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.5.0",
        "@vue/cli-plugin-eslint": "^3.5.0",
        "@vue/cli-service": "^3.5.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "compression-webpack-plugin": "^2.0.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "mockjs": "^1.0.1-beta3",
        "node-sass": "^4.9.0",
        "postcss-pxtorem": "^4.0.1",
        "sass-loader": "^7.1.0",
        "stylelint": "^10.0.1",
        "stylelint-config-standard": "^18.3.0",
        "svgtofont": "^1.1.19",
        "vue-template-compiler": "^2.5.21",
        "webpack-bundle-analyzer": "^3.3.2"
    }
}
